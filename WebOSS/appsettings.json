{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  //缓存配置
  "Cache": {
    "IsEnabled": false, // 是否启用缓存
    //缓存提供器：0、MemoryCache 1、Redis
    "Provider": 1,
    //Redis配置
    "Redis": {
      "DefaultDb": 0,
      "Prefix": "NM",
      "ConnectionString": "127.0.0.1:6379"
    }
  },
  //OSS配置
  "OSS": {
    "IsEnabled": false, // 是否启用OSS
    //存储提供器：0、本地存储 1、七牛 2、阿里云
    "Provider": 2,
    //阿里云存储
    "Aliyun": {
      //地域节点
      "Endpoint": "oss-cn-shanghai.aliyuncs.com",
      //访问令牌ID
      "AccessKeyId": "",
      //访问令牌密钥
      "AccessKeySecret": "",
      //存储空间名称
      "BucketName": "",
      //自定义访问域名
      "Domain": ""
    }
  }
}
